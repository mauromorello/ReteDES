}

.hidden{
    display:none !important;
}

.rg_widget_alert{
color : rgba(120,40,40,1);
font-weight: bold;    
}

.rg_widget .rg_w_header{

font-size:1.3em;
font-weight: bold;
padding-top : .5em;
padding-left: 10px;
padding-right :.5em;
padding-bottom:.5em;
  
}
.rg_widget .rg_w_header:hover{
cursor :move;


background-image: -webkit-gradient(
    linear,
    left top,
    left bottom,
    color-stop(0, rgb(216,216,216)),
    color-stop(0.9, rgb(253,253,253))
 
);

background-image: -moz-linear-gradient(
    center top,
    rgb(216,216,216) 0%,
    rgb(253,253,253) 90%
);

}


.rg_widget .rg_w_body{

padding-top : .5em;
padding-left: 10px;
padding-right :.5em;
padding-bottom:1em;
    
}

.rg_widget .rg_w_footer{
font-size:.8em;    
padding-top : .5em;
padding-left: 10px;
padding-right :.5em;
padding-bottom:.5em;
font-style: oblique;    
}

.rg_widget .rg_w_settings{
font-size:.8em;    
padding-top : .5em;
padding-left: 10px;
padding-right :.5em;
padding-bottom:.5em;
border: 2px solid  rgba(220,220,220,1);
-moz-border-radius-topleft: 6px; -webkit-border-top-left-radius: 6px; border-top-left-radius: 6px; -moz-border-radius-topright: 6px; -webkit-border-top-right-radius: 6px; border-top-right-radius: 6px;
-moz-border-radius-bottomleft: 6px; -webkit-border-bottom-left-radius: 6px; border-bottom-left-radius: 6px; -moz-border-radius-bottomright: 6px; -webkit-border-bottom-right-radius: 6px; border-bottom-right-radius: 6px;
background-color: #E0E0E0;
}

.rg_widget .rg_w_settings label{
    display:inline-block;
    width:15em;
}


